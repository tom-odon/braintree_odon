{
  "name": "Braintree Spring Example Hosted Fields",
  "description": "An expansion on the example Braintree integration for Spring (Java)",
  "keywords": ["braintree", "java", "spring"],
  "website": "http://www.tomodon.com",
  "repository": "https://github.com/tom-odon/braintree_odon",
  "success_url": "/",
  "env": {
    "BT_ENVIRONMENT": {
      "description": "Run against Braintree sandbox environment",
      "required": true,
      "value": "sandbox"
    },
    "BT_MERCHANT_ID": {
      "description": "Your Braintree Merchant ID",
      "required": true
    },
    "BT_PUBLIC_KEY": {
      "description": "Your Braintree Public Key",
      "required": true
    },
    "BT_PRIVATE_KEY": {
      "description": "Your Braintree Private Key",
      "required": true
    }
  }
}
